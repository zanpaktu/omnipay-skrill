        "gateway",
        "merchant",
        "skrill",
        "omnipay",
        "pay",
        "payment"
    ],
    "homepage": "https://github.com/zanpaktu/omnipay-skrill",
    "license": "MIT",
    "authors": [
        {
            "name": "Joao Dias",
            "email": "joao.dias@cherrygroup.com"
        },
        {
            "name": "Leo Camangian",
            "email": "camangianleo@gmail.com"
        }
    ],
    "require": {
        "omnipay/common": "^3.0"
    },
    "autoload": {
        "psr-0": {
            "Omnipay\\Skrill\\": "src/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3",
        "omnipay/tests": "^3",
        "phpro/grumphp": "^0.14"
